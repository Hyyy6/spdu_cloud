{
    "version": "2.0",
    "logging": {
        "fileLoggingMode": "always",
        "logLevel": {
            "SPDUAPI": "Debug",
            "Utils": "Debug",
            "Function": "Debug"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "maxTelemetryItemsPerSecond": 20,
                "excludedTypes": "Exception"
            }
        }
    },
    "extensions": {
        "http": {
            "routePrefix": "",
            "customHeaders": {
                "Connection": "close"
            }
        }
    }
}